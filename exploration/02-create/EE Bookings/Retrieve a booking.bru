meta {
  name: Retrieve a booking
  type: http
  seq: 2
}

get {
  url: {{baseUrl}}/booking/{{bookingId}}
  body: none
}

headers {
  Accept: */*
}

assert {
  res.body.firstname: isJson
  res.body.depositpaid: isFalsy
  res.body.firstname: isString
  res.status: 200
  res: isJson
  res.body: isJson
  res.body: isString
  res.body.totalprice: isJson
  res.body.totalprice: isNumber
  res.body.depositpaid: isJson
  res.body.depositpaid: isBoolean
  res.body.bookingdates: isJson
  res.body.bookingdates: isString
}

tests {
  const data = res.getBody();
  
  test("should be an object", function() {
    const data = res.getBody();
    expect(data).to.be.a('object');
  });
  
}
